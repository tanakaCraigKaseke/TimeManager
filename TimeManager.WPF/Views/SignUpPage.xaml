<UserControl x:Class="TimeManager.WPF.Views.SignUpPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TimeManager.WPF.Views"
             xmlns:signUpShared="clr-namespace:TimeManager.WPF.Shared.Forms.SignUp"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid VerticalAlignment="Center" HorizontalAlignment="Center">
        <StackPanel>
            <TextBlock Text="Sign Up" FontSize="20" Foreground="Black"></TextBlock>

            <signUpShared:SignUpForm />
        </StackPanel>
    </Grid>
</UserControl>
